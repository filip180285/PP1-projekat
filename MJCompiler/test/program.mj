program test3Faza


{
	void main() int niz[];

	{
		niz = new int[5];
		niz[0] = 0; niz[1] = 1; niz[2] = 2; niz[3] = 3; niz[4] = 4;
		print(niz[3], 3); // 3
		niz[3]@4; // niz[3] = niz[(3 + 4) % 5] = niz[2] = 2
		print(niz[3], 3); // 2
		
		print(niz[4], 3); // 4
		niz[4]@-4; // niz[4] = niz[(4 - 4) % 5] = niz[0] = 0
		print(niz[4], 3); // 2
	}
}